<?xml version="1.0" encoding="euc-kr"?>
<interface>
    
    <wmq>
        <transport>client</transport> <!-- 기본모드 bind, client -->
        <channel_property>SYSTEM.DEF.SVRCONN</channel_property>
        
        <!-- Queue 매니저 정보 -->
        <queuemanagers>
            <!-- instanceName(-DHOST_NAME) : 적용 컨테이너
                 bindname(큐메니저명) : BIND 모드인경우 접속 큐메니저명
                 clientnode(연계노드정보): 장애시 참조할 연계노드명 
                 매니저명 획득 방법 instanceName검색
                    순위1. 완전한 인스턴스명
                    순위2. 인스턴스명 앞자리(_구분맨앞[호스트명])
                    순위3. 사용불가
                 receivenodes : 클라이언트모드인 경우 선언한 노드들의 consumer로 등록한다.
                 transport: 해당패턴의 매니저 연결모드 없으면 기본모드의 client
                 sendnodes : 클라이언트 모드의 경우 전송처리시에 사용할 노드 정보들 세팅이 되어 있지 않으면 receivenodes정보를 세팅 
                            receivenodes 노드 정보도 없으면 clientnode 가 세팅 된다.
             -->
            <!-- 개발서버 -->
            <queuemanager instanceName="szca01_mqm01_dev" bindname="NMCD1" clientnode="MQ_NMCD1"/>
            <queuemanager instanceName="szca01_mqm02_dev" bindname="NMCD2" clientnode="MQ_NMCD2"/>
            <queuemanager instanceName="szca01" bindname="NMCD1" clientnode="MQ_NMCD1"/>
            <queuemanager instanceName="szcbat" bindname="NMCD1" clientnode="MQ_NMCD1"/>
            
            <queuemanager instanceName="szca01_mqm11_dev"  bindname="NMCD1" clientnode="MQ_NMCD2" receivenodes="MQ_NMCD1,MQ_NMCD2" sendnodes="MQ_NMCD2,MQ_NMCD1"/>
             <!-- bindname="NMCD1" clientnode="MQ_NMCD1"/> -->
             <!-- <queuemanager instanceName="szca01_mqm11_dev" bindname="NMCD1" clientnode="MQ_NMCD2"/> -->
             
            <!--  테스트서버 
            <queuemanager instanceName="szca01" bindname="NMCT1" clientnode="MQ_NMCT1"/>
            <queuemanager instanceName="szca02" bindname="NMCT2" clientnode="MQ_NMCT2"/>
            <queuemanager instanceName="szca03" bindname="NMCT3" clientnode="MQ_NMCT3"/>
            <queuemanager instanceName="szca04" bindname="NMCT4" clientnode="MQ_NMCT4"/>-->
        </queuemanagers>
        
