<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="false"
 monitoring="autodetect" dynamicConfig="true">

    <diskStore path="user.home"/>
    <!--
    <cacheManagerEventListenerFactory class="" properties=""/>
    <sizeOfPolicy maxDepth="10" maxDepthExceededBehavior="continue"/>
    <defaultCache
            maxElementsInMemory="10000"
            eternal="false"
            timeToIdleSeconds="90"
            timeToLiveSeconds="90"
            overflowToDisk="false"
            diskSpoolBufferSizeMB="30"
            maxElementsOnDisk="10000000"
            diskExpiryThreadIntervalSeconds="120"
            memoryStoreEvictionPolicy="LRU"
            >
            <persistence strategy="localTempSwap"/>
    </defaultCache>
    -->
    
    <sizeOfPolicy maxDepthExceededBehavior="abort"/>
    
    <!-- SCP Framework에서 전체적인 메모리를 관리하기 위해서 정의 반드시 있어야 함. -->
    <!-- Code와 관련된 EhcacheKey -->
    <cache name="code"
           eternal="true"
           overflowToDisk="false"
           maxBytesLocalHeap="1M"
           memoryStoreEvictionPolicy="LRU"
            />

    <cache name="devicemachine"
           eternal="true"
           overflowToDisk="false"
           maxBytesLocalHeap="1M"
           memoryStoreEvictionPolicy="LRU"
            />

    <!-- LabelService 와 관련된 EhcacheKey -->
    <cache name="label"
           eternal="true"
           overflowToDisk="false"
           maxBytesLocalHeap="1M"
           memoryStoreEvictionPolicy="LRU"
            />

    <!-- MenuAccCtrlService 와 관련된 EhcacheKey -->
    <cache name="menu-access-control"
           eternal="true"
           overflowToDisk="false"
           maxBytesLocalHeap="1M"
           memoryStoreEvictionPolicy="LRU"
            />

    <!-- MessageService 와 관련된 EhcacheKey -->
    <cache name="message"
           eternal="true"
           overflowToDisk="false"
           maxBytesLocalHeap="1M"
           memoryStoreEvictionPolicy="LRU"
            />

     <!-- NScreenService 와 관련된 EhcacheKey -->
    <cache name="nscreen"
           eternal="true"
