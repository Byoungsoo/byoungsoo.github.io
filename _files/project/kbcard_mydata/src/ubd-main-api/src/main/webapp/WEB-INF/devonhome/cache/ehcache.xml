            />

    <!-- TranAccCtrlService 와 관련된 EhcacheKey -->
    <cache name="transaction-access-control"
           eternal="true"
           overflowToDisk="false"
           maxBytesLocalHeap="10M"
           memoryStoreEvictionPolicy="LRU"
            />

     <!-- TranCtrlService 와 관련된 EhcacheKey -->
    <cache name="transaction-control"
           eternal="true"
           overflowToDisk="false"
           maxBytesLocalHeap="1M"
           memoryStoreEvictionPolicy="LRU"
        />
    
    <cache name="transaction-business-parameter"
